<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kalia.bhaskar.terminalandroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/terminal3"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Black" >
        <activity
            android:name="kalia.bhaskar.terminalandroid.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="COMMANDS_LIST"
            android:label="ATERMINAL/COMMANDS"
            android:theme="@android:style/Theme.Dialog"
            
            ></activity>
        <activity android:name="about"
            android:label="ABOUT"
            android:theme="@android:style/Theme.Dialog"></activity>
        <activity android:name="username"
            android:label="ATERMINAL/USER_NAME"
            android:theme="@android:style/Theme.Dialog"></activity>
    </application>

</manifest>
